{
    "name": "vaibhav/laravel-log-enhancer",
    "description": "Make debugging easier by adding more data to your laravel logs",
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*",
        "monolog/monolog": "^1.23"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vaibhav Mehta",
            "email": "vaibhavmht225@gmail.com"
        }
    ],
    "minimum-stability": "",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}

